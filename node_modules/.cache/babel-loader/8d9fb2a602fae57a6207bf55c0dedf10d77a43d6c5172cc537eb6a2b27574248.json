    function prevMonth() {\n      currentMonth.value = subMonths(currentMonth.value, 1);\n    }\n\n    function nextMonth() {\n      currentMonth.value = addMonths(currentMonth.value, 1);\n    }\n\n    function toggleModal() {\n      showModal.value = !showModal.value;\n    }\n\n    function closeModalOnOverlay() {\n      closeModal();\n    }\n\n    function selectDate(date) {\n  selectedDate.value = date;\n  console.log(\"Selected Date:\", selectedDate.value);\n  closeModal();\n  showRecordPage.value = true;\n}\n\n    function toggleWalkRecord() {\n      showWalkRocord.value = !showWalkRocord.value;\n    }\n    function closeWalkRecord() {\n      showWalkRocord.value = false;\n    }\n\nfunction isOctober1st(date) {\n  const formattedDate = format(date, \"MMÏõî ddÏùº\");\n  const isOctober1st = formattedDate === \"10Ïõî 01Ïùº\";\n  console.log(`Date ${formattedDate} is October 1st: ${isOctober1st}`);\n  return isValid(date) && isOctober1st;\n}\n\n    return {\n      currentDate: computed(() =>\n        format(currentDate.value, \"yyyyÎÖÑ MMÏõî ddÏùº\")\n      ),\n      showModal,\n      currentMonth: computed(() => format(currentMonth.value, \"yyyyÎÖÑ MMÏõî\")),\n      daysInMonth,\n      daysOfWeek,\n      showCalendarModal,\n      prevMonth,\n      nextMonth,\n      closeModal,\n      toggleModal,\n      closeModalOnOverlay,\n      showRecordPage,\n      selectDate,\n      selectedDate,\n      toggleWalkRecord,\n      showWalkRocord,\n      closeWalkRecord,\n      isOctober1st,\n    };\n  },\n});\n</script>\n\n<style scoped>\n.NewWalkRecordPage {\n  width: 100%;\n  height: 100%;\n  overflow-y: auto;\n  background-color: #f1f8ff;\n}\n/* top */\n\n/* middle */\n.NewWalkRecordPage .middle {\n  position: relative;\n  width: 100%;\n  height: 100vh;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: flex-start;\n}\n.NewWalkRecordPage .middle_top {\n  width: 100%;\n  height: 5vh;\n  background: #70b4cb;\n  display: flex;\n  justify-c